{
   "name": "vue-wswg-editor",
   "version": "0.0.10",
   "type": "module",
   "main": "./dist/vue-wswg-editor.es.js",
   "module": "./dist/vue-wswg-editor.es.js",
   "typings": "./dist/types/index.d.ts",
   "types": "./dist/types/index.d.ts",
   "exports": {
      ".": {
         "import": "./dist/vue-wswg-editor.es.js",
         "require": "./dist/vue-wswg-editor.es.js",
         "types": "./dist/types/index.d.ts"
      },
      "./PageRenderer": {
         "import": "./src/components/PageRenderer/PageRenderer.vue",
         "types": "./src/components/PageRenderer/PageRenderer.vue"
      },
      "./vite-plugin": {
         "import": "./dist/vite-plugin.js",
         "require": "./dist/vite-plugin.js",
         "types": "./src/vite-plugin.ts"
      },
      "./style.css": "./dist/style.css"
   },
   "files": [
      "dist",
      "src",
      "types"
   ],
   "scripts": {
      "dev": "concurrently \"vite build --watch\" \"vite build --config vite-plugin.config.ts --watch\"",
      "build": "vite build && vite build --config vite-plugin.config.ts && vue-tsc --declaration --emitDeclarationOnly",
      "preview": "vite preview",
      "docs:dev": "vitepress dev docs",
      "docs:build": "vitepress build docs",
      "docs:preview": "vitepress preview docs",
      "test:unit": "vitest",
      "test:unit:ui": "vitest --ui",
      "test:unit:ci": "vitest run",
      "--PROJECT--": "",
      "tscheck": "vue-tsc --build --force",
      "prettier": "prettier --check .",
      "prettier-fix": "prettier . --write",
      "lint": "eslint . --quiet",
      "lint-fix": "npm run lint --fix --quiet",
      "stylelint": "stylelint \"src/**/*.css\" \"**/*.scss\" \"**/*.vue\"",
      "stylelint-fix": "stylelint \"src/**/*.css\" \"**/*.scss\" \"**/*.vue\" --fix",
      "format": "npm run tscheck && npm run prettier && npm run lint && npm run stylelint",
      "format-fix": "npm run tscheck && npm run prettier-fix && npm run lint-fix && npm run stylelint-fix"
   },
   "dependencies": {
      "@headlessui/vue": "^1.7.23",
      "@heroicons/vue": "^2.2.0",
      "@vueuse/core": "^14.0.0",
      "sass": "^1.89.2",
      "sortablejs": "^1.15.6",
      "vue": "^3.4.0"
   },
   "devDependencies": {
      "@types/jsdom": "^21.1.7",
      "@types/node": "^24.0.8",
      "@types/sortablejs": "^1.15.9",
      "@vitejs/plugin-vue": "^5.2.4",
      "@vitest/eslint-plugin": "^1.3.5",
      "@vitest/ui": "^2.1.9",
      "@vue/eslint-config-prettier": "^10.2.0",
      "@vue/eslint-config-typescript": "^14.5.1",
      "@vue/test-utils": "^2.4.6",
      "@vue/tsconfig": "^0.7.0",
      "autoprefixer": "^10.4.21",
      "concurrently": "^9.2.1",
      "eslint": "^9.30.0",
      "eslint-plugin-tailwindcss": "^3.18.2",
      "eslint-plugin-vue": "^9.33.0",
      "jsdom": "^26.1.0",
      "postcss": "^8.5.6",
      "postcss-import": "^16.1.0",
      "postcss-scss": "^4.0.9",
      "prettier": "^3.6.2",
      "sass": "^1.89.2",
      "stylelint": "^16.21.0",
      "stylelint-config-recess-order": "^7.1.0",
      "stylelint-config-standard": "^38.0.0",
      "stylelint-config-standard-scss": "^15.0.1",
      "stylelint-config-standard-vue": "^1.0.0",
      "tailwindcss": "^3.4.10",
      "typescript": "~5.8.3",
      "vite": "^5.4.19",
      "vitepress": "^1.6.4",
      "vitepress-demo-plugin": "^1.5.1",
      "vitest": "^2.1.9",
      "vue-tsc": "^2.2.12",
      "yup": "^1.7.1"
   },
   "peerDependencies": {
      "vue": "^3.4.0"
   }
}
